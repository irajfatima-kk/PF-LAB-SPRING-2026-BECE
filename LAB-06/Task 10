#include <stdio.h>
int main() {
    int score, distinction=0, pass=0, fail=0;
    printf("Enter scores (-1 to stop):\n");
    while(1) {
        scanf("%d", &score);
        if(score == -1) break;
        if(score >= 75) distinction++;
        else if(score >= 50) pass++;
        else fail++;
    }
    printf("Distinction: %d\nPass: %d\nFail: %d\n", distinction, pass, fail);
    return 0;
}
