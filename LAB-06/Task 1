#include <stdio.h>
int main() {
    int i, status, present = 0;
    for(i = 1; i <= 30; i++) {
        printf("Enter attendance for student %d (1=Present, 0=Absent): ", i);
        scanf("%d", &status);
        if(status == 1) present++;
    }
    printf("Total students present: %d\n", present);
    return 0;
}
